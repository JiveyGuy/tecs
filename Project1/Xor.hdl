/**
 *  Exclusive-or gate:
 *  out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=a, out=na);
    Nand(a=b, b=b, out=nb);

    Nand(a=a, b=nb, out=nanb);
    Nand(a=b, b=na, out=nbna);

    Nand(a=nanb, b=nbna, out=out);
}
